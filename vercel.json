{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "buildCommand": "cd frontend && bun install && bun run build",
  "devCommand": "cd frontend && bun run dev",
  "installCommand": "cd frontend && bun install",
  "framework": null,
  "outputDirectory": "frontend/build/client",
  "routes": [
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}

